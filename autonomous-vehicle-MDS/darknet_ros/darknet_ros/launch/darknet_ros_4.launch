<?xml version="1.0" encoding="utf-8"?>

<launch>
  <!-- Console launch prefix -->
  <arg name="launch_prefix" default=""/>
  
  <!-- Config and weights folder. -->
  <arg name="yolo_weights_path"          default="$(find darknet_ros)/yolo_network_config/weights"/>
  <arg name="yolo_config_path"           default="$(find darknet_ros)/yolo_network_config/cfg"/>

  <!-- Load parameters -->
  <rosparam command="load" ns="down_looking_camera/darknet_ros" file="$(find darknet_ros)/config/ros.yaml"/>
  <rosparam command="load" ns="down_looking_camera/darknet_ros" file="$(find darknet_ros)/config/yolov3_delivery.yaml"/>

  <!-- Start darknet and ros wrapper -->
  <node  ns="down_looking_camera" pkg="darknet_ros" type="darknet_ros" name="darknet_ros" output="screen" launch-prefix="$(arg launch_prefix)">
    <param name="weights_path"                              value="$(arg yolo_weights_path)" />
    <param name="config_path"                               value="$(arg yolo_config_path)" />
  </node>


  <!--node pkg="darknet_ros" type="vision_second_S.py" name="sign_pub" output="screen"-->
  <node pkg="darknet_ros" type="vision_jewoo_quick.py" name="sign_pub" output="screen">
  </node>


  <!--<node name="republish" type="republish" pkg="image_transport" output="screen" 	args="compressed in:=/front_camera/image_raw raw out:=/camera/image_raw" /> -->
  </launch>

